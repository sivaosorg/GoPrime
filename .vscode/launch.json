    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch JOB APP",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/job/main.go",
            "args": [
                "start_server_job",
                "./../../config/conf-local.yaml"
            ]
        },
        {
            "name": "Launch REST APP",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/api/main.go",
            "args": [
                "start_server",
                "./../../config/conf-local.yaml",
                "./../../config/conf-params.yaml",
            ]
        }
    ]
}